{
  "name": "transpile-iconify",
  "version": "0.0.1",
  "description": "Transpile Iconify is a command-line tool that transforms SVG files into JSON format for use with Iconify.",
  "author": "0fatihyildiz",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "start": "pnpm tsx ./src/index.ts",
    "build": "pnpm exec pkgroll"
  },
  "keywords": ["svg", "json", "iconify", "cli", "transpiler"],
  "license": "MIT",
  "dependencies": {
    "cleye": "^1.3.2",
    "glob": "^11.0.0",
    "pkgroll": "^2.5.0",
    "tsx": "^4.19.1",
    "xml2js": "^0.6.2"
  }
}