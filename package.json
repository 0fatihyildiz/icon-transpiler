{
  "name": "iconify-transpile",
  "version": "0.1.5-rc",
  "description": "Transform SVG files into JSON format for Iconify",
  "author": "0fatihyildiz",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": {
    "iconify-transpile": "./dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "build": "tsc"
  },
  "keywords": [
    "svg",
    "json",
    "iconify",
    "cli",
    "transpiler"
  ],
  "license": "MIT",
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "@types/node": "^18.0.0",
    "@vitest/coverage-v8": "3.0.9",
    "typescript": "^5.0.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@iconify/tools": "^3.0.7",
    "cheerio": "1.0.0-rc.12",
    "cleye": "^1.3.2",
    "glob": "^11.0.0",
    "pkgroll": "^2.5.0",
    "tsx": "^4.19.1",
    "xml2js": "^0.6.2"
  }
}
